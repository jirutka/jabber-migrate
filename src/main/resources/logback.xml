<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <contextName>roster-migrate</contextName>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%-5level] %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>


    <logger name="cz.rdc.devel.jabber.migrate" level="DEBUG" />

    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>

</configuration>
